      <TextField fx:id="lnText" layoutX="220.0" layoutY="200.0" />
      <TextField fx:id="emailText" layoutX="220.0" layoutY="340.0" />
      <TextField fx:id="phText" layoutX="220.0" layoutY="480.0" />
      <ComboBox fx:id="months" layoutX="152.0" layoutY="620.0" prefHeight="26.0" prefWidth="105.0" promptText="January" />
      <ComboBox fx:id="days" layoutX="266.0" layoutY="620.0" prefHeight="26.0" prefWidth="68.0" promptText="1" />
      <ComboBox fx:id="years" layoutX="340.0" layoutY="620.0" prefWidth="150.0" promptText="2001" />
   </AnchorPane>
   <AnchorPane minHeight="0.0" minWidth="0.0" style="-fx-background-color: #E5E7E9;">
      <Label layoutX="167.0" layoutY="10.0" text="Account Details" underline="true">
         <font>
            <Font name="Glockenspiel Regular" size="25.0" />
         </font>
      </Label>
      <Button fx:id="registerB" layoutX="14.0" layoutY="750.0" mnemonicParsing="false" onAction="#register" text="Register" />
      <Label layoutX="10.0" layoutY="60.0" text="Username">
         <font>
            <Font name="RowdyHeavy Regular" size="18.0" />
         </font>
      </Label>
      <Label layoutX="10.0" layoutY="200.0" text="Password">
         <font>
            <Font name="RowdyHeavy Regular" size="18.0" />
         </font>
      </Label>
      <Label layoutX="10.0" layoutY="340.0" text="Confirm Password">
         <font>
            <Font name="RowdyHeavy Regular" size="18.0" />
         </font>
      </Label>
      <CheckBox layoutX="376.0" layoutY="200.0" mnemonicParsing="false" text="Show Password" />
      <TextField fx:id="usernameText" layoutX="190.0" layoutY="60.0" />
      <PasswordField fx:id="passText" layoutX="190.0" layoutY="200.0" />
