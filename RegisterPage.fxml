         <font>
            <Font name="Glockenspiel Regular" size="25.0" />
         </font>
      </Label>
      <Label layoutX="10.0" layoutY="60.0" text="First Name" textAlignment="CENTER">
         <font>
            <Font name="RowdyHeavy Regular" size="18.0" />
         </font>
      </Label>
      <Label layoutX="10.0" layoutY="200.0" text="Last Name" textAlignment="CENTER">
         <font>
            <Font name="RowdyHeavy Regular" size="18.0" />
         </font>
      </Label>
      <Label layoutX="10.0" layoutY="340.0" text="Email" textAlignment="CENTER">
         <font>
            <Font name="RowdyHeavy Regular" size="18.0" />
         </font>
      </Label>
      <Label layoutX="10.0" layoutY="480.0" text="Phone Number" textAlignment="CENTER">
         <font>
            <Font name="RowdyHeavy Regular" size="18.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="620.0" text="Date of Birth" textAlignment="CENTER">
         <font>
            <Font name="RowdyHeavy Regular" size="18.0" />
